@article{brownPayingAttentionAudiovisual2019b,
  title = {``{{Paying}}'' Attention to Audiovisual Speech: {{Do}} Incongruent Stimuli Incur Greater Costs?},
  shorttitle = {``{{Paying}}'' Attention to Audiovisual Speech},
  author = {Brown, Violet A. and Strand, Julia F.},
  date = {2019-08-01},
